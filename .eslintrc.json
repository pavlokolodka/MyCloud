{
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021
    },
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "import/order": ["error", {"groups": [["builtin", "external"]]}]
    }
  }
  